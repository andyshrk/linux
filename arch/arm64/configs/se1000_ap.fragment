# enable microchip MCP251XFD SPI2CAN
CONFIG_CAN_MCP251XFD=y

# whware rtc device
CONFIG_RTC_LIB=y
CONFIG_RTC_DRV_SD3078=y

# ti ads7138 A/D converter
