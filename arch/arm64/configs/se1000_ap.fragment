CONFIG_SENSORS_ADS7138=y

# NXP 88w9098 WIFI/BT module
CONFIG_WLAN_VENDOR_NXP=y
CONFIG_88W9098=m

# sound codec es8316
CONFIG_SND_SOC_ES8316=y