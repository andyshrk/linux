/ {
#define SE1000_DEFAULT_TEMPERATURE_ACTIVE			90000	/* 95 celsius */
#define SE1000_DEFAULT_TEMPERATURE_HOT				110000	/* 115 celsius */
#define SE1000_DEFAULT_TEMPERATURE_CRITICAL			125000	/* 125 celsius */
#define SE1000_DEFAULT_TEMPERATURE_HYSTERESIS		2000	/* 2 celsius */

#define SE1000_DEFAULT_POLLING_DELAY		1000	/* 1 second */
#define SE1000_DEFAULT_DELAY_PASSIVE		5000	/* 5 second */

#define SE1000_DEFAULT_STEP		4
#define SE1000_DEFAULT_RANGE(upper, lower)	((upper) - (lower))

#define SE1000_DEFAULT_ACTIVE_RANGE	\
	SE1000_DEFAULT_RANGE(SE1000_DEFAULT_TEMPERATURE_HOT, SE1000_DEFAULT_TEMPERATURE_ACTIVE)

#define SE1000_DEFAULT_ACTIVE_STEP	(SE1000_DEFAULT_ACTIVE_RANGE / SE1000_DEFAULT_STEP)
#define SE1000_DEFAULT_ACTIVE_POINT(n)	\
	(SE1000_DEFAULT_TEMPERATURE_ACTIVE + SE1000_DEFAULT_ACTIVE_STEP * (n))

#define SE1000_DEFAULT_HOT_RANGE	\
	SE1000_DEFAULT_RANGE(SE1000_DEFAULT_TEMPERATURE_CRITICAL, SE1000_DEFAULT_TEMPERATURE_HOT)

#define SE1000_DEFAULT_HOT_STEP	(SE1000_DEFAULT_HOT_RANGE / SE1000_DEFAULT_STEP)
#define SE1000_DEFAULT_HOT_POINT(n)	\
	(SE1000_DEFAULT_TEMPERATURE_HOT + SE1000_DEFAULT_HOT_STEP * (n))

	pvt: se1000-pvt {
		compatible = "siengine,se1000-pvt";
		interrupts = <GIC_SPI 175 IRQ_TYPE_EDGE_RISING>,
					<GIC_SPI 177 IRQ_TYPE_EDGE_RISING>;
		interrupt-names = "ts_alarmb","vm_alarmb";
		memory-region = <&thermal_reserved>;
#if ENABLE_AP_CP_SIENGINE
		rpmsg = <&rpmsg_dev_a762cm4>;
#else
		rpmsg = <&rpmsg_dev2cm4>;
#endif
		#thermal-sensor-cells = <1>;
	};

	thermal_zones: thermal-zones {
		/* gpu0 ddr1 ap1 */
		gpu0_thermal_zone: thermal_gpu0 {
			polling-delay = <SE1000_DEFAULT_POLLING_DELAY>;
			polling-delay-passive = <SE1000_DEFAULT_DELAY_PASSIVE>;
			thermal-sensors = <&pvt 0>;

			trips {
				gpu0_trip_active: gpu0-trip-active {
					temperature = <SE1000_DEFAULT_TEMPERATURE_ACTIVE>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_active0: gpu0-trip-active0 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(0)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_active1: gpu0-trip-active1 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(1)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_active2: gpu0-trip-active2 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(2)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_active3: gpu0-trip-active3 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(3)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_hot: gpu0-trip-hot {
					temperature = <SE1000_DEFAULT_TEMPERATURE_HOT>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				gpu0_trip_critical: gpu0-trip-critical {
					temperature = <SE1000_DEFAULT_TEMPERATURE_CRITICAL>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "critical";
				};
			};

#if ENABLE_AP_CP_SIENGINE
			cooling-maps {
				map0-active0 {
					trip = <&gpu0_trip_active0>;
					cooling-device = <&gpu0 0 1>, <&gpu0_core 2 2>;
				};

				map0-active1 {
					trip = <&gpu0_trip_active1>;
					cooling-device = <&gpu0 1 2>, <&gpu0_core 4 4>;
				};

				map0-active2 {
					trip = <&gpu0_trip_active2>;
					cooling-device = <&gpu0 2 3>, <&gpu0_core 6 6>;
				};

				map0-active3 {
					trip = <&gpu0_trip_active3>;
					cooling-device = <&gpu0 3 THERMAL_NO_LIMIT>, <&gpu0_core 8 8>;
				};

				map1 {
					trip = <&gpu0_trip_hot>;
					cooling-device = <&gpu0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
							 <&gpu0_core 8 8>;
				};

				map2 {
					trip = <&gpu0_trip_critical>;
					cooling-device = <&gpu0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
							 <&gpu0_core 8 8>;
				};
			};
#else
			cooling-maps {
				map0-active3 {
					trip = <&gpu0_trip_active3>;
					cooling-device =
						<&cpu0 0 1>,
						<&cpu1 0 1>;
				};

				map1 {
					trip = <&gpu0_trip_hot>;
					cooling-device =
						<&cpu0 2 THERMAL_NO_LIMIT>,
						<&cpu1 2 THERMAL_NO_LIMIT>;
				};

				map2 {
					trip = <&gpu0_trip_critical>;
					cooling-device =
						<&cpu0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
						<&cpu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
				};
			};
#endif
		};

		/* smp(perf) nup0/1 sfc cisp pcie */
		top_thermal_zone: thermal_top {
			polling-delay = <SE1000_DEFAULT_POLLING_DELAY>;
			polling-delay-passive = <SE1000_DEFAULT_DELAY_PASSIVE>;
			thermal-sensors = <&pvt 1>;

			trips {
				top_trip_active: top-trip-active {
					temperature = <SE1000_DEFAULT_TEMPERATURE_ACTIVE>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				top_trip_active0: top-trip-active0 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(0)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				top_trip_active1: top-trip-active1 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(1)>;
