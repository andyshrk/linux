					type = "active";
				};

				top_trip_hot: top-trip-hot {
					temperature = <SE1000_DEFAULT_TEMPERATURE_HOT>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				top_trip_critical: top-trip-critical {
					temperature = <SE1000_DEFAULT_TEMPERATURE_CRITICAL>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "critical";
				};
			};

			cooling-maps {
				map0-active0 {
					trip = <&top_trip_active0>;
					cooling-device =
						<&npu1 0 1>,
						<&devfreq_gdc 0 1>,
						<&devfreq_cisp 0 1>;
				};

				map0-active1 {
					trip = <&top_trip_active1>;
					cooling-device =
						<&npu1 1 2>,
						<&devfreq_gdc 1 2>,
						<&devfreq_cisp 1 2>;
				};

				map0-active2 {
					trip = <&top_trip_active2>;
					cooling-device =
						<&npu1 2 3>,
						<&devfreq_gdc 2 3>,
						<&devfreq_cisp 2 3>;
				};

				map0-active3 {
					trip = <&top_trip_active3>;
					cooling-device =
						<&npu1 3 THERMAL_NO_LIMIT>,
						<&devfreq_gdc 3 THERMAL_NO_LIMIT>,
						<&devfreq_cisp 3 THERMAL_NO_LIMIT>;
				};

				map1 {
					trip = <&top_trip_hot>;
					cooling-device =
						<&npu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
						<&devfreq_gdc THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
						<&devfreq_cisp THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
				};

				map2 {
					trip = <&top_trip_critical>;
					cooling-device =
						<&npu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
						<&devfreq_gdc THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
						<&devfreq_cisp THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
				};
			};
		};

		/* saf gpu1 mdp vpu gpu2 */
