				};

				map0-active3 {
					trip = <&saf_trip_active3>;
					cooling-device = <&gpu1 0 1>, <&gpu1_core 3 3>;
				};

				map1 {
					trip = <&saf_trip_hot>;
					cooling-device = <&gpu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
							 <&gpu1_core 3 3>;
				};

				map2 {
					trip = <&saf_trip_critical>;
					cooling-device = <&gpu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
							 <&gpu1_core 3 3>;
				};
			};
#endif
		};

		/* ddr0 ap0 usb */
		ap0_thermal_zone: thermal_ap0 {
			polling-delay = <SE1000_DEFAULT_POLLING_DELAY>;
			polling-delay-passive = <SE1000_DEFAULT_DELAY_PASSIVE>;
			thermal-sensors = <&pvt 3>;

			trips {
				ap0_trip_active: ap0-trip-active {
					temperature = <SE1000_DEFAULT_TEMPERATURE_ACTIVE>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				ap0_trip_active0: ap0-trip-active0 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(0)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				ap0_trip_active1: ap0-trip-active1 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(1)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
				};

				ap0_trip_active2: ap0-trip-active2 {
					temperature = <SE1000_DEFAULT_ACTIVE_POINT(2)>;
					hysteresis = <SE1000_DEFAULT_TEMPERATURE_HYSTERESIS>;
					type = "active";
