			no-map;
			reg = <0x0 0x86c00000 0x0 0x800000>;
		};

		hsm_rp_shmem_reserved: hsm_rp_shmem_reserved@0x87400000{
			no-map;
			reg = <0 0x87400000 0 0x0800000>;
			// 8M
		};

		wifi_mhi_reserved: wifi_mhi_region {
			compatible = "shared-dma-pool";
			no-map;
			reg = <0x0 0x87C00000 0x0 0x2000000>;
		};

		wifi_cnss_reserved: wifi_cnss_region {
			compatible = "shared-dma-pool";
			no-map;
			reg = <0x0 0x89C00000 0x0 0x1000000>;
