/ {
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/* global autoconfigured region for contiguous allocations */
		linux,cma {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x0 0x60000000>;
			linux,cma-default;
		};

		efuse_reserved:@0x58020000 {
			no-map;
			reg = <0 0x58020000 0 0x10000>;
			// 64K
		};

		rp_reserved: rp@0x7C000000 {
			no-map;
			reg = <0 0x7C000000 0 0x4000000>;
			// 64M
		};

		vnet_reserved: vnet_reserved@0x83400000 {
			no-map;
			reg = <0 0x83400000 0 0x800000>, /* 8M for AP-CP VNET */
			      <0 0x83C00000 0 0x800000>, /* 8M for CP-RP VNET */
			      <0 0x84400000 0 0x800000>; /* 8M for AP-RP VNET */
		};

		/* warning : don't modify this memory zone by yourself, total 16MB */
		selink_reserved: selink@0x84C00000 {
			no-map;
			reg = <0x0 0x84C00000 0x0 0x01000000>; /* 16M */
		};

		drm_shmem_reserved:@0x85C00000 {
			no-map;
			reg = <0 0x85C00000 0 0x0800000>;
			// 8M
		};

		hsm_ap_shmem_reserved: hsm_ap_shmem_reserved@0x86400000 {
			no-map;
